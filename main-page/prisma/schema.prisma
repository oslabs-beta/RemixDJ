// This is your Prisma schema file
generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "mongodb"
  url      = env("DATABASE_URL")
}

model Feedback {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @default(now())
  name       String
  content    String
}

model Team {
  id      String   @id @default(auto()) @map("_id") @db.ObjectId
  createdAt  DateTime @default(now())
  name       String
  content    String
}